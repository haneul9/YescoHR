{
  "name": "zui5_yescohr",
  "description": "",
  "scripts": {
    "start": "ui5 serve -o index.html",
    "build": "ui5 build --clean-dest",
    "build-phr3": "ui5 build --clean-dest --config ./ui5-phr3.yaml",
    "deploy": "ui5 build --clean-dest && npx nwabap upload",
    "deploy-phr3": "ui5 build --clean-dest --config ./ui5-phr3.yaml && npx nwabap upload"
  },
  "devDependencies": {
    "@ui5/cli": "^2.14.14",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-ui5": "^0.1.0",
    "nwabap-ui5uploader": "^1.2.0",
    "prettier": "^2.6.2",
    "ui5-middleware-simpleproxy": "^0.7.6"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-simpleproxy"
    ],
    "ui5linter": {
      "usage": {
        "TagAttributeLinter": true,
        "PublicMemberLinter": false,
        "UnusedMemberLinter": false,
        "WrongParametersLinter": false
      }
    }
  }
}
