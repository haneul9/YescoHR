{
  "_version": "1.17.0",

  "sap.app": {
    "id": "sap.ui.yesco",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json"
  },

  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://detail-view",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "rootView": {
      "id": "app",
      "viewName": "sap.ui.yesco.view.App",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.71.28",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ui.yesco.i18n.i18n",
          "supportedLocales": ["ko", "en"],
          "fallbackLocale": ""
        }
      }
    },
    "resources": {
      "css": [
        { "uri": "css/webfonts/pretendard-subset.css" },
        { "uri": "css/style.css" },
        { "uri": "css/placeholder.css" },
        { "uri": "css/util/font-size.css" },
        { "uri": "css/util/font-weight.css" },
        { "uri": "css/util/line-height.css" },
        { "uri": "css/util/margin.css" },
        { "uri": "css/util/padding.css" },
        { "uri": "css/util/border.css" },
        { "uri": "css/util/width-height.css" },
        { "uri": "localService/RevealGrid.css" }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.ui.yesco.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "ehrHome",
          "target": "home"
        },
        {
          "pattern": "congratulation",
          "name": "congratulation",
          "target": "congratulation"
        },
        {
          "pattern": "m/congratulation",
          "name": "congratulationMobile",
          "target": "m/congratulation"
        },
        {
          "pattern": "congratulation/congDetail/{oDataKey}",
          "name": "congDetail",
          "target": "congDetail"
        },
        {
          "pattern": "m/congratulation/congDetail/{oDataKey}",
          "name": "congDetailMobile",
          "target": "m/congDetail"
        },
        {
          "pattern": "attendance",
          "name": "attendance",
          "target": "attendance"
        },
        {
          "pattern": "components",
          "name": "commonComponents",
          "target": "components"
        },
        {
          "pattern": "timeline",
          "name": "timeline",
          "target": "timeline"
        },
        {
          "pattern": "ninebox",
          "name": "ninebox",
          "target": "ninebox"
        }
      ],
      "targets": {
        "home": {
          "viewId": "home",
          "viewPath": "sap.ui.yesco.view.home",
          "viewName": "Home",
          "viewLevel": 1
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        },
        "congratulation": {
          "viewId": "congratulation",
          "viewPath": "sap.ui.yesco.view.congratulation",
          "viewName": "congratulation",
          "viewLevel": 2
        },
        "congDetail": {
          "viewId": "congDetail",
          "viewPath": "sap.ui.yesco.view.congratulation",
          "viewName": "CongDetail",
          "viewLevel": 2
        },
        "m/congratulation": {
          "viewId": "congratulationMobile",
          "viewPath": "sap.ui.yesco.view.congratulation.m",
          "viewName": "Congratulation",
          "viewLevel": 2
        },
        "m/congDetail": {
          "viewId": "congDetailMobile",
          "viewPath": "sap.ui.yesco.view.congratulation.m",
          "viewName": "CongDetail",
          "viewLevel": 2
        },
        "attendance": {
          "viewId": "attendanceList",
          "viewPath": "sap.ui.yesco.view.attendance",
          "viewName": "List",
          "viewLevel": 2
        },
        "components": {
          "viewId": "components",
          "viewPath": "sap.ui.yesco.view.zample",
          "viewName": "Components",
          "viewLevel": 1,
          "title": "Components"
        },
        "timeline": {
          "viewId": "timeline",
          "viewPath": "sap.ui.yesco.view.zample",
          "viewName": "Timeline",
          "viewLevel": 1,
          "title": "Timeline Sample"
        },
        "ninebox": {
          "viewId": "ninebox",
          "viewPath": "sap.ui.yesco.view.zample",
          "viewName": "Ninebox",
          "viewLevel": 1,
          "title": "Ninebox Sample"
        }
      }
    }
  }
}
