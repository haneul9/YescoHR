<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:custom="sap.ui.yesco.control">
  <HBox justifyContent="SpaceBetween" class="header-wrap">
    <custom:HeaderTitleCountMessage titlePath="i18n>LABEL_00168" countPath="/listInfo/totalCount" class="title" />
    <HBox>
      <HBox class="status-wrap">
        <ObjectStatus text="{i18n>LABEL_48002} {/listInfo/Count20}" icon="sap-icon://circle-task-2" state="Indication02" />
        <ObjectStatus text="{i18n>LABEL_48004} {/listInfo/Count30}" icon="sap-icon://circle-task-2" state="Indication03" />
        <ObjectStatus text="{i18n>LABEL_48005} {/listInfo/Count40}" icon="sap-icon://circle-task-2" state="Indication05" />
      </HBox>
      <Button icon="sap-icon://excel-attachment" text="Excel" press=".onPressExcelDownload" busyIndicatorDelay="0" busy="{/contentsBusy/table}">
        <layoutData>
          <FlexItemData styleClass="button-excel-attachment" />
        </layoutData>
      </Button>
    </HBox>
  </HBox>
</core:FragmentDefinition>