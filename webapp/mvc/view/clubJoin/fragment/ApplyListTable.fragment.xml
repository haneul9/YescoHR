<core:FragmentDefinition xmlns="sap.m"
  xmlns:table="sap.ui.table"
  xmlns:core="sap.ui.core">
  <table:Table id="clubTable" class="row-link" rows="{/List}" visibleRowCount="{/listInfo/rowCount}" width="100%" selectionMode="None" columnHeaderHeight="45" rowHeight="44" busyIndicatorDelay="0" busy="{/busy}" noData="{i18n>MSG_00001}" cellClick=".onSelectRow">
    <table:rowSettingsTemplate>
      <table:RowSettings highlight="{
                path: 'Lnsta',
                formatter: '.TableUtils.rowHighlight'
            }"/>
    </table:rowSettingsTemplate>
    <table:columns>
      <table:Column width="100px" sortProperty="Lnsta" filterProperty="Lnsta">
        <Label text="{i18n>LABEL_00144}"/>
        <table:template>
          <Text text="{
                        path: 'Lnsta',
                        formatter: '.TableUtils.StatusTxt'
                    }" width="100%" textAlign="Center">
            <customData>
              <core:CustomData key="status" value="Z{Lnsta}" writeToDom="true" />
            </customData>
          </Text>
        </table:template>
      </table:Column>
      <table:Column width="auto" sortProperty="Zclubtx" filterProperty="Zclubtx">
        <Label text="{i18n>LABEL_14007}"/>
        <table:template>
          <Text text="{Zclubtx}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="100px" sortProperty="Mcnt" filterProperty="Mcnt">
        <Label text="{i18n>LABEL_14008}"/>
        <table:template>
          <Text text="{Mcnt}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="100px" sortProperty="Headnm" filterProperty="Headnm">
        <Label text="{i18n>LABEL_14009}"/>
        <table:template>
          <Text text="{Headnm}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="100px" sortProperty="Leadnm" filterProperty="Leadnm">
        <Label text="{i18n>LABEL_14010}"/>
        <table:template>
          <Text text="{Leadnm}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="150px" sortProperty="Memberyn" filterProperty="Memberyn">
        <Label text="{i18n>LABEL_14011}"/>
        <table:template>
          <Text text="{Memberyn}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="300px" sortProperty="Period" filterProperty="Period">
        <Label text="{i18n>LABEL_14012}"/>
        <table:template>
          <Text text="{Period}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="150px" sortProperty="Coaidtx" filterProperty="Coaidtx">
        <Label text="{i18n>LABEL_14013}"/>
        <table:template>
          <Text text="{Coaidtx}" textAlign="Center" wrapping="false" width="100%" class="tableRow_Text"/>
        </table:template>
      </table:Column>
      <table:Column width="150px" sortProperty="Appda" filterProperty="Appda">
        <Label text="{i18n>LABEL_00266}"/>
        <table:template>
          <Text text="{
                        path: 'Appda',
                        type: 'sap.ui.yesco.mvc.model.type.Date',
                        formatOptions: {
                            pattern: 'yyyy.MM.dd'
                        }
                    }" textAlign="Center" width="100%" wrapping="false" class="tableRow_Text"/>
        </table:template>
      </table:Column>
    </table:columns>
  </table:Table>
</core:FragmentDefinition>