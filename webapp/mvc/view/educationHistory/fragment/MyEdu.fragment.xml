<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:custom="sap.ui.yesco.control">
  <VBox class="hCardBox overview-box educationHistory-overview" busyIndicatorDelay="0" busy="{/busy}">
    <VBox>
      <custom:HeaderTitleCountMessage titlePath="i18n>LABEL_31002" class="title" />
      <Text text="{ path: '/Total/Zyear', formatter: '.thisYear' }" class="sapUiSmallMarginBottom" />
      <HBox class="mt-50-px educationHistory-overview-content">
        <VBox width="33%" class="overview-info">
          <Label design="Bold" text="{i18n>LABEL_31006}" />
          <ObjectStatus text="{ path: '/Total/Total', formatter: '.formatNumber' }" state="Indication05" />
        </VBox>
        <VBox width="33%" class="overview-info">
          <Label design="Bold" text="{i18n>LABEL_31007}" />
          <HBox alignItems="Baseline">
            <ObjectStatus text="{ path: '/Total/NonComplete', formatter: '.formatNumber' }" state="Indication05" />
          </HBox>
        </VBox>
        <VBox class="overview-info">
          <Label design="Bold" text="{i18n>LABEL_31008}" />
          <HBox alignItems="Baseline">
            <ObjectStatus text="{ path: '/Total/Complete', formatter: '.formatNumber' }" state="Indication05" />
          </HBox>
        </VBox>
      </HBox>
    </VBox>
  </VBox>
</core:FragmentDefinition>