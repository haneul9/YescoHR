<core:FragmentDefinition xmlns="sap.ui.table"
  xmlns:m="sap.m"
  xmlns:core="sap.ui.core">
  <m:Dialog showHeader="false" contentWidth="1600px" contentHeight="520px" draggable="true">
    <m:content>
      <m:VBox class="dialog-content-table">
        <m:HBox class="header-wrap">
          <m:HBox class="header" alignItems="Center">
            <m:Title level="H3" text="{i18n>LABEL_10113}" class="sapUiSmallMarginEnd" />
          </m:HBox>
        </m:HBox>
        <m:HBox id="ChangeFappContainer">
          <Table selectionMode="None" rows="{/changeDialog/list}" noData="{i18n>MSG_00001}" busy="{/changeDialog/busy}">
            <layoutData>
              <m:FlexItemData minWidth="280px" />
            </layoutData>
            <customData>
              <core:CustomData key="Zzappun2" value="{Zzappun2}" />
            </customData>
            <columns class="SmallMarginTop">
              <Column width="auto">
                <m:Label text="{i18n>LABEL_00209}"/>
                <template>
                  <m:Text text="{ path: 'Zzappee', type: 'sap.ui.yesco.mvc.model.type.Pernr' }" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_00210}"/>
                <template>
                  <m:Text text="{Ename}" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_00215}"/>
                <template>
                  <m:Text text="{Zzjikgbt}" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_10094}"/>
                <template>
                  <m:Text text="{Zzappuntx2}" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_00224}"/>
                <template>
                  <m:Text text="{Orgtx}" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_10003}"/>
                <template>
                  <m:Text text="{ path: 'Zapgme', type: 'sap.ui.yesco.mvc.model.type.Decimal', formatOptions: { maxFractionDigits: 2 } }" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_10004}"/>
                <template>
                  <m:Text text="{ path: 'Zapgma', type: 'sap.ui.yesco.mvc.model.type.Decimal', formatOptions: { maxFractionDigits: 2 } }" width="100%" textAlign="Center" wrapping="false" />
                </template>
              </Column>
              <Column width="auto">
                <m:Label text="{i18n>LABEL_10078}"/>
                <template>
                  <m:ComboBox width="100%" selectedKey="{Fapp}" editable="{/isActive}" items="{ path: '/changeDialog/item', templateShareable: false }">
                    <core:Item key="{ValueEid}" text="{ValueText}" />
                  </m:ComboBox>
                </template>
              </Column>
            </columns>
          </Table>
        </m:HBox>
      </m:VBox>
    </m:content>
    <m:buttons>
      <m:Button type="Emphasized" text="{i18n>LABEL_00103}" visible="{/isActive}" press=".onPressChangeFappDialogSave"/>
      <m:Button text="{i18n>LABEL_00115}" class="cancel" press=".onPressChangeFappDialogClose" />
    </m:buttons>
  </m:Dialog>
</core:FragmentDefinition>