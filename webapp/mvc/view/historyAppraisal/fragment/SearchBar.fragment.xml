        <HBox class="form-bar" alignItems="Center">
            <Label text="{i18n>LABEL_15005}" />
            <DatePicker displayFormat="yyyy" valueFormat="yyyy" value="{/history/search/Zyear}" change=".onPressSearch" />
        </HBox>
    </HBox>
</core:FragmentDefinition>