              <Label text="{i18n>LABEL_13009}"/>
              <HBox alignContent="SpaceBetween" justifyContent="End">
                <Text text="{ path: '/summary/list/0/Bet03', type: 'sap.ui.yesco.mvc.model.type.Currency' }"/>
              </HBox>
              <Label text="{i18n>LABEL_13040}"/>
              <HBox alignContent="SpaceBetween" justifyContent="End">
                <Text text="{ path: '/summary/list/0/Bet04', type: 'sap.ui.yesco.mvc.model.type.Currency' }" class="font-18-px font-bold"/>
              </HBox>
              <Label text="{i18n>LABEL_13011}"/>
              <HBox alignContent="SpaceBetween" justifyContent="End">
                <Text text="{ path: '/summary/list/0/Bet05', type: 'sap.ui.yesco.mvc.model.type.Currency' }"/>
              </HBox>
              <Label text="{i18n>LABEL_13038}"/>
              <HBox alignContent="SpaceBetween" justifyContent="End">
                <ObjectStatus text="{ path: '/summary/list/0/Bet06', type: 'sap.ui.yesco.mvc.model.type.Currency' }" state="Indication05" class="font-16-px"/>
              </HBox>
              <Label text="{i18n>LABEL_13013}"/>
              <HBox alignContent="SpaceBetween" justifyContent="End">
                <ObjectStatus text="{ path: '/summary/list/0/Bet07', type: 'sap.ui.yesco.mvc.model.type.Currency' }" state="Indication02" class="font-16-px"/>
