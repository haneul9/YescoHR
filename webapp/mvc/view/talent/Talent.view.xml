<mvc:View controllerName="sap.ui.yesco.mvc.controller.talent.Talent"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:custom="sap.ui.yesco.control">
  <Page title="{i18n>LABEL_35001}" showHeader="false">
    <VBox class="contents-header">
      <HBox justifyContent="SpaceBetween" class="breadcrumbs">
        <custom:Breadcrumbs />
        <HBox>
          <Button type="Emphasized" class="sapUiTinyMarginEnd" text="{i18n>LABEL_35002}" press=".onDialog" />
          <Button type="Emphasized" class="sapUiTinyMarginEnd" text="{i18n>LABEL_35003}" press=".onCompareDialogM" />
          <Button type="Emphasized" class="sapUiTinyMarginEnd" text="{i18n>LABEL_35004}" press=".onSearchDialog" />
          <Button text="{i18n>LABEL_00131}" icon="sap-icon://print" press=".onPagePrint" class="sapUiTinyMarginEnd" />
          <Button text="{i18n>LABEL_00132}" icon="sap-icon://sys-help" press=".onPressHelp" visible="{= !!${menuModel>/current/showHelp} }" />
        </HBox>
      </HBox>
    </VBox>
    <VBox class="contents">
      <core:Fragment fragmentName="sap.ui.yesco.mvc.view.talent.fragment.SearchConditions" type="XML" />
      <core:Fragment fragmentName="sap.ui.yesco.mvc.view.talent.fragment.SearchResults" type="XML" />
    </VBox>
  </Page>
</mvc:View>