<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:custom="sap.ui.yesco.control">
	<Page showHeader="false" busyIndicatorDelay="0" busy="{/contentsBusy/page}">
		<VBox class="contents-header">
			<HBox justifyContent="SpaceBetween" class="breadcrumbs">
				<custom:Breadcrumbs />
				<HBox class="button-group">
					<Button type="Emphasized" text="{i18n>LABEL_00105}" busyIndicatorDelay="0" busy="{/contentsBusy/button}" press=".onPressNew" />
					<Button icon="sap-icon://print" text="{i18n>LABEL_00131}" class="sapUiTinyMarginEnd" press=".onPagePrint" />
					<Button icon="sap-icon://sys-help" text="{i18n>LABEL_00132}" press=".onPressHelp" visible="{= !!${menuModel>/current/showHelp} }" />
				</HBox>
			</HBox>
		</VBox>
		<VBox class="contents" justifyContent="SpaceBetween">
			<HBox class="vCardBox" height="80px" busyIndicatorDelay="0" busy="{/contentsBusy/search}">
				<HBox class="form-bar" alignItems="Center">
					<Label text="{i18n>LABEL_00237}" />
					<ComboBox items="{/entry/Werks}" selectedKey="{/search/Werks}">
						<core:Item text="{Pbtxt}" key="{Werks}" />
					</ComboBox>
					<Label text="{i18n>LABEL_48001}" />
					<custom:DateRangeSelection dateValue="{ path: '/search/Begda', type: 'sap.ui.yesco.mvc.model.type.Date' }" secondDateValue="{ path: '/search/Endda', type: 'sap.ui.yesco.mvc.model.type.Date' }" class="sapIcon_Date" />
					<Button text="{i18n>LABEL_00100}" icon="/sap/public/bc/ui2/zui5_yescohr/images/icon_search03.png" iconDensityAware="false" press=".onPressSearch" />
				</HBox>
			</HBox>
			<VBox class="vCardBox" busyIndicatorDelay="0" busy="{/contentsBusy/table}">
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.documentSubmitMng.fragment.OverviewTableHeader" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.documentSubmitMng.fragment.OverviewTable" type="XML" />
			</VBox>
		</VBox>
	</Page>
</core:FragmentDefinition>