<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <HBox justifyContent="SpaceBetween" class="header-wrap">
        <HBox class="header">
            <Title level="H3" text="{i18n>LABEL_00168}" class="sapUiSmallMarginEnd" />
            <Text text="total {/listInfo/totalCount}" />
        </HBox>
        <HBox>
            <HBox class="status-wrap">
                <ObjectStatus text="{i18n>LABEL_48002} {/listInfo/Count10}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication04" />
                <ObjectStatus text="{i18n>LABEL_48003} {/listInfo/Count20}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication02" />
                <ObjectStatus text="{i18n>LABEL_48004} {/listInfo/Count30}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication03" />
                <ObjectStatus text="{i18n>LABEL_48005} {/listInfo/Count40}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication05" />
            </HBox>
            <Button icon="sap-icon://excel-attachment" text="Excel" press=".onPressExcelDownload" busyIndicatorDelay="0" busy="{/contentsBusy/table}" />
        </HBox>
    </HBox>
</core:FragmentDefinition>