<mvc:View controllerName="sap.ui.yesco.mvc.controller.healthCare.HealthCareDetail"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="sap.ui.yesco.control">
  <Page showHeader="false">
    <content>
      <VBox class="contents-header">
        <HBox justifyContent="SpaceBetween" class="breadcrumbs">
          <custom:Breadcrumbs />
          <HBox>
            <Button text="{i18n>LABEL_00108}" class="sapUiTinyMarginEnd" type="Emphasized" press=".onFixBtn" visible="{= ${/FormData/Chanable} === 'X' &amp;&amp; !${/Fixed} }" />
            <Button text="{i18n>LABEL_00103}" class="sapUiTinyMarginEnd" type="Emphasized" press=".onSaveBtn" visible="{/Fixed}" />
            <Button text="{i18n>LABEL_00146}" class="sapUiTinyMarginEnd" press=".onNavBack" />
            <Button text="{i18n>LABEL_00131}" class="sapUiTinyMarginEnd" icon="sap-icon://print" press=".FragmentEvent.onPagePrint" />
            <Button text="{i18n>LABEL_00132}" icon="sap-icon://sys-help" />
          </HBox>
        </HBox>
      </VBox>
      <VBox class="contents">
        <core:Fragment fragmentName="sap.ui.yesco.fragment.AppointeeBar" type="XML" />
        <core:Fragment fragmentName="sap.ui.yesco.fragment.InfoMessage" type="XML" />
        <core:Fragment fragmentName="sap.ui.yesco.mvc.view.healthCare.fragment.DetailForm" type="XML" />
      </VBox>
    </content>
  </Page>
</mvc:View>