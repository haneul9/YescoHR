<mvc:View controllerName="sap.ui.yesco.mvc.controller.healthCare.HealthCare"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="sap.ui.yesco.control">
  <Page showHeader="false">
    <content>
      <VBox class="contents-header">
        <HBox justifyContent="SpaceBetween" class="breadcrumbs">
          <custom:Breadcrumbs />
          <HBox class="button-group">
            <Button icon="sap-icon://print" press=".onPagePrint" text="{i18n>LABEL_00131}" />
            <Button icon="sap-icon://sys-help" text="{i18n>LABEL_00132}" press=".onPressHelp" visible="{= !!${menuModel>/current/showHelp} }" />
          </HBox>
        </HBox>
      </VBox>
      <VBox class="contents">
        <HBox justifyContent="SpaceAround">
          <core:Fragment fragmentName="sap.ui.yesco.fragment.AppointeeBox" type="XML" />
          <core:Fragment fragmentName="sap.ui.yesco.mvc.view.healthCare.fragment.MyHealthHistory" type="XML" />
          <core:Fragment fragmentName="sap.ui.yesco.mvc.view.healthCare.fragment.SearchCondition" type="XML" />
        </HBox>
        <VBox class="vCardBox">
          <core:Fragment fragmentName="sap.ui.yesco.fragment.OverviewTableHeader" type="XML" />
          <core:Fragment fragmentName="sap.ui.yesco.mvc.view.healthCare.fragment.ApplyListTable" type="XML" />
        </VBox>
      </VBox>
    </content>
  </Page>
</mvc:View>