<mvc:View controllerName="sap.ui.yesco.mvc.controller.nightduty.List"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="sap.ui.yesco.control">
	<Page showHeader="false">
		<VBox class="contents-header">
			<HBox justifyContent="SpaceBetween" class="breadcrumbs">
				<custom:Breadcrumbs />
				<HBox>
					<Button text="{i18n>LABEL_00121}" press=".onClick" type="Emphasized" />
				</HBox>
			</HBox>
		</VBox>
		<VBox class="contents">
			<HBox justifyContent="SpaceAround" class="sapUiMediumMarginBottom">
				<core:Fragment fragmentName="sap.ui.yesco.fragment.AppointeeBox" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.nightduty.fragment.Summary" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.nightduty.fragment.SearchCondition" type="XML" />
			</HBox>
			<VBox class="vCardBox">
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.nightduty.fragment.OverviewTable" type="XML" />
			</VBox>
		</VBox>
	</Page>
</mvc:View>