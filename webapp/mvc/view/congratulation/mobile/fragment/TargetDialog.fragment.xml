<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Dialog title="{i18n>LABEL_02023}" contentWidth="calc(100% - 32px)" horizontalScrolling="false" class="congrat-condol-target-list-dialog">
    <content>
      <List items="{/TargetList}" class="congrat-condol-target-list">
        <CustomListItem type="Active" press=".TargetClick">
          <HBox>
            <Text text="{Atext}" wrapping="false" />
            <Text text="{Zname}" wrapping="false" />
            <Text text="{Regno}" wrapping="false" />
          </HBox>
        </CustomListItem>
      </List>
    </content>
    <endButton>
      <Button text="{i18n>LABEL_00115}" press=".onClick" class="button-tertiary" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>