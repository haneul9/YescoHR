<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:table="sap.ui.table">
  <Dialog id= "targetSettingsDialog" title="{i18n>LABEL_02023}" contentWidth="calc(100% - 32px)">
    <content>
      <table:Table id="targetTable" width="100%" rows="{/TargetList}" visibleRowCount="{/TargetListRowCount}" selectionMode="None" rowHeight="44" columnHeaderHeight="45" columnHeaderVisible="false" cellClick=".TargetClick" class="mt-16-px mx-11-px mb-1-px">
        <table:columns>
          <table:Column width="20%">
            <table:template>
              <Text text="{Atext}" width="100%" textAlign="Center" wrapping="false" />
            </table:template>
          </table:Column>
          <table:Column width="30%">
            <table:template>
              <Text text="{Zname}" width="100%" textAlign="Center" wrapping="false" />
            </table:template>
          </table:Column>
          <table:Column width="auto">
            <table:template>
              <Text text="{Regno}" width="100%" textAlign="Center" wrapping="false" />
            </table:template>
          </table:Column>
        </table:columns>
      </table:Table>
    </content>
    <endButton>
      <Button text="{i18n>LABEL_00115}" press=".onClick" class="button-tertiary" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>