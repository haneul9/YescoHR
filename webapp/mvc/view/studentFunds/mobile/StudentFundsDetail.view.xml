<mvc:View controllerName="sap.ui.yesco.mvc.controller.studentFunds.mobile.StudentFundsDetail"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="sap.ui.yesco.control"
  xmlns:mobile="sap.ui.yesco.control.mobile">
  <mobile:Page navButtonPress=".onNavBack" showFooter="{= !'30,40,50,60'.split(',').includes(${/FormData/ZappStatAl}) }">
    <VBox class="contents">
      <mobile:ScrollContainer>
        <VBox class="wrap">
          <HBox class="table-toolbar">
            <HBox class="title">
              <custom:Title text="{i18n>LABEL_00129}" />
            </HBox>
          </HBox>
          <mobile:DetailCSSGrid gridTemplateColumns="2fr 3fr">
            <Label text="{i18n>LABEL_03012}" />
            <Text text="{/FormData/Zzobjpstx}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" />
            <custom:SingleComboBox items="{/AppTarget}" selectedKey="{/FormData/Zzobjps}" change=".onChangeTarget" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="required">
              <core:Item text="{Znametx}" key="{Zzobjps}" />
            </custom:SingleComboBox>

            <Label text="{i18n>LABEL_03014}" />
            <Text text="{/FormData/Slarttx}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" />
            <custom:SingleComboBox items="{/AcademicSort}" selectedKey="{/FormData/Slart}" change=".onChangeSchool" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="required">
              <core:Item text="{Ztext}" key="{Zcode}" />
            </custom:SingleComboBox>

            <Label text="{i18n>LABEL_03006}" />
            <Text text="{/FormData/Grdsptx}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" />
            <custom:SingleComboBox items="{/GradeList}" selectedKey="{/FormData/Grdsp}" change=".onChangeGrade" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="required">
              <core:Item text="{Ztext}" key="{Zcode}" />
            </custom:SingleComboBox>

            <Label text="{i18n>LABEL_03007}" />
            <Text text="{/FormData/Divcdtx}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" />
            <custom:SingleComboBox items="{/QuarterList}" selectedKey="{/FormData/Divcd}" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="required">
              <core:Item text="{Ztext}" key="{Zcode}" />
            </custom:SingleComboBox>

            <Label text="{i18n>LABEL_03013}" class="white-space-pre-wrap" />
            <Text text="{/FormData/Cnttx}" class="d-flex flex-middle" />

            <Label text="{i18n>LABEL_03031}" />
            <Input value="{/FormData/Schtx}" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="required" />

            <Label text="{i18n>LABEL_03032}" visible="{= !!${/MajorInput} }" />
            <Input value="{/FormData/Majnm}" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" visible="{= !!${/MajorInput} }" />

            <Label text="{i18n>LABEL_03017}" />
            <Text text="{/FormData/Zyeartx}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" />
            <custom:SingleComboBox items="{/FundsYears}" selectedKey="{/FormData/Zyear}" change=".onChangeYear" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }">
              <core:Item text="{Ztext}" key="{Zcode}" />
            </custom:SingleComboBox>

            <Label text="{i18n>LABEL_03033}" />
            <CheckBox select=".onSelectForeign" selected="{= !!${/FormData/Forsch} ? true : false }" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" />

            <Label text="{i18n>LABEL_03019}" />
            <Input value="{ path: '/FormData/ZbetEntr', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03020}" />
            <Input value="{ path: '/FormData/ZbetMgmt', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03021}" />
            <Input value="{ path: '/FormData/ZbetClass', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03022}" />
            <Input value="{ path: '/FormData/ZbetExer', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03023}" />
            <Input value="{ path: '/FormData/ZbetSuf', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03026}" />
            <Input value="{ path: '/FormData/ZbetEtc', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveChangeCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03034}" class="white-space-pre-wrap" />
            <Input value="{ path: '/FormData/ZbetTotl', type: 'sap.ui.yesco.mvc.model.type.Currency' }" width="100%" editable="false" class="currency">
              <customData>
                <core:CustomData key="padding" value="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' ? 'O' : 'X'}" writeToDom="true" />
              </customData>
            </Input>

            <Label text="{i18n>LABEL_03025}" />
            <Input value="{ path: '/FormData/ZbetShip', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".onSchoCost" maxLength="10" width="100%" editable="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" class="currency" />

            <Label text="{i18n>LABEL_03027}" class="align-top" />
            <VBox class="multi-line">
              <Input value="{ path: '/FormData/ZpayAmt', type: 'sap.ui.yesco.mvc.model.type.Currency' }" width="100%" editable="false" class="currency">
                <customData>
                  <core:CustomData key="padding" value="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' ? 'O' : 'X'}" writeToDom="true" />
                </customData>
              </Input>
              <ObjectStatus text="{i18n>MSG_03002}" state="Indication05" visible="{/LimitAmountMSG}" />
            </VBox>

            <Label text="{i18n>LABEL_03029}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" class="align-top" />
            <Text text="{/FormData/Zresn}" visible="{= !!${/FormData/ZappStatAl} &amp;&amp; ${/FormData/ZappStatAl} !== '10' }" class="multi-line" />
          </mobile:DetailCSSGrid>
          <core:Fragment fragmentName="sap.ui.yesco.fragment.CommonAttachFile" type="XML" />
          <core:Fragment fragmentName="sap.ui.yesco.fragment.mobile.ApplyInfo" type="XML" />
          <core:Fragment fragmentName="sap.ui.yesco.fragment.mobile.ApprovalDetails" type="XML" />
        </VBox>
      </mobile:ScrollContainer>
    </VBox>
    <mobile:footer>
      <OverflowToolbar visible="{= !'30,40,50,60'.split(',').includes(${/FormData/ZappStatAl}) }">
        <Button text="{i18n>LABEL_00122}" type="Reject" press=".onCancelBtn" visible="{= ${/FormData/ZappStatAl} === '20' }" />
        <Button text="{i18n>LABEL_00110}" type="Reject" press=".onDeleteBtn" visible="{= ${/FormData/ZappStatAl} === '10' || ${/FormData/ZappStatAl} === '45' }" />
        <Button text="{i18n>LABEL_00120}" type="Emphasized" press=".onRewriteBtn" visible="{= ${/FormData/ZappStatAl} === '45' }" />
        <Button text="{i18n>LABEL_00104}" type="Emphasized" press=".onSaveBtn" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" />
        <Button text="{i18n>LABEL_00121}" type="Emphasized" press=".onApplyBtn" visible="{= !${/FormData/ZappStatAl} || ${/FormData/ZappStatAl} === '10' }" />
      </OverflowToolbar>
    </mobile:footer>
  </mobile:Page>
</mvc:View>