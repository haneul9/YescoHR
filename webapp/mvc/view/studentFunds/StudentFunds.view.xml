<mvc:View controllerName="sap.ui.yesco.mvc.controller.studentFunds.StudentFunds"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="sap.ui.yesco.control">
	<Page showHeader="false">
		<VBox class="contents-header">
			<HBox justifyContent="SpaceBetween" class="breadcrumbs">
				<custom:Breadcrumbs />
				<HBox>
					<Button text="{i18n>LABEL_00121}" type="Emphasized" press=".onClick" class="sapUiTinyMarginEnd" />
					<Button icon="sap-icon://print" press=".FragmentEvent.onPagePrint" text="{i18n>LABEL_00131}" class="sapUiTinyMarginEnd" />
					<Button icon="sap-icon://sys-help" text="{i18n>LABEL_00132}" />
				</HBox>
			</HBox>
		</VBox>
		<VBox class="contents">
			<HBox justifyContent="SpaceAround" class="sapUiMediumMarginBottom">
				<core:Fragment fragmentName="sap.ui.yesco.fragment.TargetInfo" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.studentFunds.fragment.ThisYearFunds" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.studentFunds.fragment.SearchCondition" type="XML" />
			</HBox>
			<VBox class="vCardBox">
				<core:Fragment fragmentName="sap.ui.yesco.fragment.OverviewTableHeader" type="XML" />
				<core:Fragment fragmentName="sap.ui.yesco.mvc.view.studentFunds.fragment.ApplyListTable" type="XML" />
			</VBox>
		</VBox>
	</Page>
</mvc:View>