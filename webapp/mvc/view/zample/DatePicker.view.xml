<mvc:View controllerName="sap.ui.yesco.mvc.controller.zample.DatePicker"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="sap.ui.yesco.control">
	<HBox class="sapUiSmallMargin">
		<VBox>
			<Title text="sap.m.DatePicker" class="component-title" />
			<DatePicker showFooter="true" width="400px" />
		</VBox>
		<VBox>
			<Title text="sap.ui.yesco.control.DatePicker" class="component-title" />
			<custom:DatePicker width="140px" />
		</VBox>
		<VBox>
			<Title text="sap.ui.yesco.control.DateRangeSelection" class="component-title" />
			<custom:DateRangeSelection width="230px" />
		</VBox>
	</HBox>
</mvc:View>
