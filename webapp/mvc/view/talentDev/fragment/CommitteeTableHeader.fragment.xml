<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:custom="sap.ui.yesco.control">
  <HBox justifyContent="SpaceBetween" class="header-wrap">
    <custom:HeaderTitleCountMessage titlePath="i18n>LABEL_43001" countPath="/committee/listInfo/totalCount" />
    <HBox>
      <HBox class="status-wrap">
        <ObjectStatus text="{i18n>LABEL_43013} {/committee/listInfo/readyCount}" icon="sap-icon://circle-task-2" />
        <ObjectStatus text="{i18n>LABEL_43014} {/committee/listInfo/progressCount}" icon="sap-icon://circle-task-2" state="Indication02" />
        <ObjectStatus text="{i18n>LABEL_43015} {/committee/listInfo/completeCount}" icon="sap-icon://circle-task-2" state="Indication05" />
      </HBox>
      <Button icon="sap-icon://excel-attachment" text="Excel" press=".onPressCommitteeExcelDownload" busyIndicatorDelay="0" busy="{/busy/Button}" />
    </HBox>
  </HBox>
</core:FragmentDefinition>