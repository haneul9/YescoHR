<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:custom="sap.ui.yesco.control">
    <VBox class="portlet portlet-box" visible="{/contents/A01/busy}">
        <custom:Placeholder />
    </VBox>
    <VBox class="portlet portlet-box portlet-m21" visible="{= !${/contents/A01/busy} }">
        <HBox class="portlet-header">
            <Title level="H2" text="{i18n>LABEL_28002}" />
            <Image src="asset/image/icon_human_green.svg" width="12px" class="ml-8-px" />
            <Image src="asset/image/icon_human_orange.svg" width="12px" class="ml-3-px" />
            <Text text="">
                <layoutData>
                    <FlexItemData growFactor="1" />
                </layoutData>
            </Text>
            <Button visible="{hasLink}" icon="sap-icon://add" type="Transparent" class="icon-button portlet-link-icon" />
        </HBox>
        <HBox class="portlet-body">
            <VBox width="30%">
                <ObjectStatus text="{/contents/A01/data/Cnt01}" state="Indication05" class="mt-15-px font-3-rem" active="true" press=".onPressCount">
                    <customData>
                        <core:CustomData key="Headty" value="{/contents/A01/Headty}" />
                        <core:CustomData key="Discod" value="{/contents/A01/data/Cnt01C}" />
                    </customData>
                </ObjectStatus>
                <ObjectStatus text="{i18n>LABEL_01213} : {/contents/A01/data/Cnt02}" state="Indication05" class="mt-13-px" active="true" press=".onPressCount">
                    <customData>
                        <core:CustomData key="Headty" value="{/contents/A01/Headty}" />
                        <core:CustomData key="Discod" value="{/contents/A01/data/Cnt02C}" />
                    </customData>
                </ObjectStatus>
                <ObjectStatus text="{i18n>LABEL_01212} : {/contents/A01/data/Cnt03}" state="Indication05" class="mt-7-px" active="true" press=".onPressCount">
                    <customData>
                        <core:CustomData key="Headty" value="{/contents/A01/Headty}" />
                        <core:CustomData key="Discod" value="{/contents/A01/data/Cnt03C}" />
                    </customData>
                </ObjectStatus>
            </VBox>
            <VBox class="ml-5-px">
                <layoutData>
                    <FlexItemData growFactor="1" />
                </layoutData>
                <HBox width="100%" height="22px" justifyContent="End" alignItems="Center" class="border-bottom">
                    <Text text="{i18n>LABEL_01211}" textAlign="Center" width="100%">
                        <layoutData>
                            <FlexItemData baseSize="20%" />
                        </layoutData>
                    </Text>
                    <Text text="{i18n>LABEL_01213}" textAlign="Center" width="100%">
                        <layoutData>
                            <FlexItemData baseSize="20%" />
                        </layoutData>
                    </Text>
                    <Text text="{i18n>LABEL_01212}" textAlign="Center" width="100%">
                        <layoutData>
                            <FlexItemData baseSize="20%" />
                        </layoutData>
                    </Text>
                </HBox>
                <HBox width="100%" alignItems="Center" class="border-bottom">
                    <layoutData>
                        <FlexItemData growFactor="1" />
                    </layoutData>
                    <Label text="{i18n>LABEL_01214}" textAlign="Center" width="100%">
                        <layoutData>
                            <FlexItemData growFactor="1" />
                        </layoutData>
                    </Label>
                    <Text text="{ path: '/contents/A01/data/Cnt40', type: 'sap.ui.yesco.mvc.model.type.Currency' }" textAlign="Center" width="100%">
