                    </Text>
                </HBox>
                <HBox width="100%" alignItems="Center" class="border-bottom">
                    <layoutData>
                        <FlexItemData growFactor="1" />
                    </layoutData>
                    <Label text="{i18n>LABEL_01215}" textAlign="Center" width="100%">
                        <layoutData>
                            <FlexItemData growFactor="1" />
                        </layoutData>
                    </Label>
                    <Text text="{ path: '/contents/A01/data/Cnt50', type: 'sap.ui.yesco.mvc.model.type.Currency' }" textAlign="Center" width="100%">
