                    </Text>
                    <ObjectStatus text="{ path: '/contents/A01/data/Cnt61', type: 'sap.ui.yesco.mvc.model.type.Currency' }" active="true" press=".onPressCount">
                        <customData>
                            <!-- 퇴사자 직원 -->
