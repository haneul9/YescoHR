<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:custom="sap.ui.yesco.control">
  <VBox class="portlet portlet-box" visible="{/contents/A02/busy}">
    <custom:Placeholder />
  </VBox>
  <VBox class="portlet portlet-box" visible="{= !${/contents/A02/busy} }">
    <customData>
      <core:CustomData key="key" value="a02" />
    </customData>
    <HBox class="portlet-header">
      <Title level="H2" text="{i18n>LABEL_28010}" />
      <Image src="/sap/public/bc/ui2/zui5_yescohr/images/icon_human_green.svg" width="12px" class="ml-8-px" />
      <Text text="">
        <layoutData>
          <FlexItemData growFactor="1" />
        </layoutData>
      </Text>
      <Button visible="{hasLink}" icon="/sap/public/bc/ui2/zui5_yescohr/images/icon_window.svg" iconDensityAware="false" type="Transparent" class="icon-button portlet-link-icon" press=".onToggleExpanded" />
    </HBox>
    <VBox class="portlet-body">
      <layoutData>
        <FlexItemData growFactor="1" />
      </layoutData>
      <custom:HBox width="100%" justifyContent="Center" class="active-link" click=".onPressCount">
        <custom:customData>
          <core:CustomData key="Headty" value="{/contents/A02/Headty}" />
          <core:CustomData key="Discod" value="{/contents/A02/data/Cod01}" />
        </custom:customData>
        <Label text="{/contents/A02/data/Title01}" class="font-20-px"/>
      </custom:HBox>
      <HBox width="100%" justifyContent="SpaceBetween" alignItems="Center">
        <custom:HBox class="active-link" click=".onPressCount">
          <custom:customData>
            <core:CustomData key="Headty" value="{/contents/A02/Headty}" />
            <core:CustomData key="Discod" value="{/contents/A02/data/Cod02}" />
          </custom:customData>
          <Text text="{/contents/A02/data/Title10}" class="font-34-px lh-34-px color-01 pr-4-px" />
          <VBox class="h15">
            <Text text="{/contents/A02/data/Cnt11}&#37;" class="font-14-px color-01"/>
            <Text text="{/contents/A02/data/Cnt12}{i18n>LABEL_00159}" class="font-14-px color-01"/>
          </VBox>
        </custom:HBox>
        <custom:HBox class="active-link" click=".onPressCount">
          <custom:customData>
            <core:CustomData key="Headty" value="{/contents/A02/Headty}" />
            <core:CustomData key="Discod" value="{/contents/A02/data/Cod01}" />
          </custom:customData>
          <Label text="{/contents/A02/data/Cnt01}{i18n>LABEL_00159}, {/contents/A02/data/Cnt02}&#37;" class="font-27-px" />
        </custom:HBox>
        <custom:HBox class="active-link" click=".onPressCount">
          <custom:customData>
            <core:CustomData key="Headty" value="{/contents/A02/Headty}" />
            <core:CustomData key="Discod" value="{/contents/A02/data/Cod03}" />
          </custom:customData>
          <VBox class="h15">
            <Text text="{/contents/A02/data/Cnt21}&#37;" class="font-14-px color-02"/>
            <Text text="{/contents/A02/data/Cnt22}{i18n>LABEL_00159}" class="font-14-px color-02"/>
          </VBox>
          <Text text="{/contents/A02/data/Title20}" class="font-34-px lh-34-px color-02 pl-4-px" />
        </custom:HBox>
      </HBox>
      <HBox>
        <layoutData>
          <FlexItemData growFactor="1" />
        </layoutData>
        <core:HTML content='&lt;div id="employee-a02-chart-container"&gt;&lt;/div&gt;' />
      </HBox>
      <HBox justifyContent="SpaceBetween">
        <VBox>
          <Label text="{i18n>LABEL_28031}" class="font-14-px" />
          <Text text="{i18n>MSG_28001} ({/contents/A02/data/ageMGen}{i18n>LABEL_28059})" class="font-14-px ls-small" />
        </VBox>
        <VBox alignItems="End">
          <Label text="{i18n>LABEL_28032}" class="font-14-px text-right" />
          <Text text="{i18n>MSG_28002} ({/contents/A02/data/ageZGen}{i18n>LABEL_28059})" class="font-14-px text-right ls-small" />
        </VBox>
      </HBox>
    </VBox>
  </VBox>
</core:FragmentDefinition>