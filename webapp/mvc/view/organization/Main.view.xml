<mvc:View controllerName="sap.ui.yesco.mvc.controller.organization.Main"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:custom="sap.ui.yesco.control">
	<Page showHeader="false">
		<VBox class="contents-header">
			<HBox justifyContent="SpaceBetween" class="breadcrumbs">
				<custom:Breadcrumbs />
				<HBox>
					<Button icon="sap-icon://print" text="{i18n>LABEL_00131}" class="sapUiTinyMarginEnd" />
					<Button icon="sap-icon://sys-help" text="{i18n>LABEL_00132}" />
				</HBox>
			</HBox>
		</VBox>
		<FlexBox id="ChartHolder" width="100%" height="100%" alignItems="Start" justifyContent="Center" busyIndicatorDelay="0" />
	</Page>
</mvc:View>