<core:FragmentDefinition xmlns="sap.m"
    xmlns:grid="sap.ui.layout.cssgrid"
    xmlns:core="sap.ui.core"
    xmlns:custom="sap.ui.yesco.control">
    <Dialog id="DetailHisDialog" title="{i18n>LABEL_09022}" contentWidth="1100px" contentHeight="auto">
        <buttons>
            <Button text="{i18n>LABEL_00106}" type="Emphasized" press=".onHisRegBtn" visible="{= !!${/DialogData/isNew} }" />
            <Button text="{i18n>LABEL_00108}" type="Emphasized" press=".onHisUpBtn" visible="{= !${/DialogData/isNew} }" />
            <Button text="{i18n>LABEL_00115}" press=".onDialogClose" />
        </buttons>
        <content>
            <VBox class="vCardBox sapUiMediumMarginBottom">
                <grid:CSSGrid gridTemplateColumns="1fr 3fr 1fr 2fr" gridGap="1px 0" class="form-grid">
                    <Label text="{i18n>LABEL_09021}"/>
                    <VBox>
                        <custom:DateRangeSelection maxDate="{/DialogData/maxDate}" dateValue="{ path: '/DialogData/Begda', type: 'sap.ui.yesco.mvc.model.type.Date' }" secondDateValue="{ path: '/DialogData/Endda', type: 'sap.ui.yesco.mvc.model.type.Date' }" class="sapIcon_Date required" />
                    </VBox>
                    <Label text="{i18n>LABEL_09016}"/>
                    <FlexBox alignItems="Start" justifyContent="End">
                        <Input value="{ path: '/DialogData/Bet01', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveCompar" description="{i18n>LABEL_00158}"/>
                    </FlexBox>
                    <Label text="{i18n>LABEL_09020}"/>
                    <VBox>
                        <Input value="{/DialogData/Disenm}" class="required"/>
                    </VBox>
                    <Label text="{i18n>LABEL_09017}"/>
                    <FlexBox alignItems="Start" justifyContent="End">
                        <Input value="{ path: '/DialogData/Bet02', type: 'sap.ui.yesco.mvc.model.type.Currency' }" liveChange=".liveCompar" description="{i18n>LABEL_00158}"/>
                    </FlexBox>
                    <Label text="{i18n>LABEL_09014}"/>
                    <VBox>
                        <Input value="{/DialogData/Medorg}" class="required"/>
                    </VBox>
                    <Label text="{i18n>LABEL_09018}"/>
                    <FlexBox alignItems="Start" justifyContent="End">
                        <Text text="{ path: '/DialogData/Bettot', type: 'sap.ui.yesco.mvc.model.type.Currency' } 원" />
                    </FlexBox>
                    <Label text="{i18n>LABEL_09015}"/>
                    <VBox>
                        <ComboBox items="{/ReceiptType}" selectedKey="{/DialogData/Recpgb}" change=".onRecipt" class="required">
                            <core:Item text="{Ztext}" key="{Zcode}" />
                        </ComboBox>
                    </VBox>
                    <Label text="{i18n>LABEL_09019}"/>
                    <FlexBox alignItems="Start" justifyContent="End">
                        <Text text="{/DialogData/Prate} %" />
                    </FlexBox>
                    <HBox></HBox>
                    <HBox></HBox>
                    <Label text="{i18n>LABEL_09009}"/>
                    <FlexBox alignItems="Start" justifyContent="End">
                        <Text text="{ path: '/DialogData/Pybet', type: 'sap.ui.yesco.mvc.model.type.Currency' } 원" />
                    </FlexBox>
                </grid:CSSGrid>
            </VBox>
            <core:Fragment id="DialogAttFile" fragmentName="sap.ui.yesco.fragment.CommonAttachFile" type="XML" />
        </content>
    </Dialog>
</core:FragmentDefinition>