        <Column width="50px">
          <multiLabels>
            <m:Label text="{i18n>LABEL_45409}" />
            <m:Label text="{i18n>LABEL_45409}" />
          </multiLabels>
          <template>
            <m:Text text="{Number}" width="100%" textAlign="Center" wrapping="false" />
