  <!-- <m:VBox class="vCardBox" visible="{/candidate/listInfo/busy}">
    <custom:Placeholder />
  </m:VBox> -->
  <m:VBox class="vCardBox" visible="{= !${/candidate/listInfo/busy} &amp;&amp; ${/candidate/listInfo/totalCount} &gt; 0 }">
    <m:customData>
      <core:CustomData key="padding-bottom" value="{= !!${/candidate/listInfo/expand} ? 'O' : 'X' }" writeToDom="true" />
    </m:customData>
    <m:HBox class="table-toolbar" justifyContent="SpaceBetween">
      <m:customData>
        <core:CustomData key="padding-bottom" value="{= !!${/candidate/listInfo/expand} ? 'O' : 'X' }" writeToDom="true" />
      </m:customData>
      <m:HBox class="title">
        <m:Title text="{i18n>LABEL_45401}" level="H4" />
        <m:Text text="{i18n>MSG_45001}" class="desc-txt" />
      </m:HBox>
      <m:HBox class="status-wrap">
        <m:Image src="/sap/public/bc/ui2/zui5_yescohr/images/icon_YH.svg" />
        <m:Text text="{i18n>LABEL_WERKS_1000}" class="sapUiSmallMarginEnd"/>
        <m:Image src="/sap/public/bc/ui2/zui5_yescohr/images/icon_YS.svg" />
        <m:Text text="{i18n>LABEL_WERKS_2000}" class="sapUiSmallMarginEnd"/>
        <m:Image src="/sap/public/bc/ui2/zui5_yescohr/images/icon_HS.svg" />
        <m:Text text="{i18n>LABEL_WERKS_3000}" class="sapUiSmallMarginEnd"/>
        <m:Image src="/sap/public/bc/ui2/zui5_yescohr/images/icon_YI.svg" />
        <m:Text text="{i18n>LABEL_WERKS_4000}" class="sapUiSmallMarginEnd"/>
        <m:ObjectStatus text="{i18n>LABEL_45006}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication03" />
        <m:ObjectStatus text="{i18n>LABEL_45007}" class="sapUiSmallMarginEnd" icon="sap-icon://circle-task-2" state="Indication04" />
        <m:ObjectStatus text="{i18n>LABEL_45008}" icon="sap-icon://circle-task-2" state="Indication05" />
        <m:ToggleButton pressed="{/candidate/listInfo/expand}" icon="{= !!${/candidate/listInfo/expand} ? 'sap-icon://navigation-up-arrow' : 'sap-icon://navigation-down-arrow' }" iconDensityAware="false" class="circle-button" />
      </m:HBox>
    </m:HBox>
    <Table id="candidateTable" rows="{/candidate/listInfo/list}" visibleRowCount="{/candidate/listInfo/rowCount}" visible="{/candidate/listInfo/expand}" selectionMode="None" width="100%" columnHeaderHeight="45" rowHeight="127" class="scsn-candidates">
      <columns>
        <Column width="50px" hAlign="Center">
          <multiLabels>
            <m:Label text="{i18n>LABEL_45402}" />
            <m:Label text="{i18n>LABEL_45402}" />
          </multiLabels>
          <template>
            <m:Text text="{Number}" wrapping="false" />
          </template>
        </Column>
        <Column width="340px">
          <multiLabels>
            <m:Label text="{i18n>LABEL_45403}" />
            <m:Label text="{i18n>LABEL_45403}" />
          </multiLabels>
          <template>
            <f:Card>
              <f:content>
                <m:HBox>
                  <m:customData>
                    <core:CustomData key="color-type" value="{ColtyState}" writeToDom="true" />
                  </m:customData>
                  <m:Image src="{PicUrl}" press=".onPressPhoto">
                    <m:layoutData>
                      <m:FlexItemData styleClass="photo" />
                    </m:layoutData>
                  </m:Image>
                  <m:VBox>
                    <m:Title text="{Linetx1}" wrapping="true" />
                    <m:Image src="{Icon}">
                      <m:layoutData>
                        <m:FlexItemData styleClass="company-icon" />
                      </m:layoutData>
                    </m:Image>
                    <m:CheckBox selected="{= ${Chckd} === 'X' }" select=".onSelectCheckBox">
                      <m:layoutData>
                        <m:FlexItemData styleClass="compare-target-checkbox" />
                      </m:layoutData>
                    </m:CheckBox>
                    <m:Text text="{Linetx2}" />
                    <m:Text text="{Linetx3}" />
                    <m:Text text="{Linetx4}" />
                    <m:Text text="{Linetx5}" />
                  </m:VBox>
                </m:HBox>
              </f:content>
            </f:Card>
          </template>
        </Column>
        <Column sortProperty="Alltot" filterProperty="Alltot" hAlign="Center">
          <multiLabels>
            <m:Label text="{i18n>LABEL_45408}" />
            <m:Label text="{i18n>LABEL_45408}" />
          </multiLabels>
          <template>
            <m:Text text="{Alltot}" wrapping="false" class="cell-total" />
