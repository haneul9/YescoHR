<core:FragmentDefinition xmlns="sap.ui.table"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:custom="sap.ui.yesco.control">
    <m:Dialog showHeader="false" contentWidth="1400px" contentHeight="570px" draggable="true">
        <m:content>
            <m:VBox class="hCardBox mb-16-px" visible="{/dialog/busy}">
                <custom:Placeholder />
            </m:VBox>
            <m:VBox class="dialog-content-table" visible="{= !${/dialog/busy} }">
                <Table width="100%" selectionMode="None" fixedColumnCount="5" columnHeaderHeight="35" rowHeight="34" rows="{/dialog/list}" visibleRowCount="{/dialog/rowCount}" noData="{i18n>MSG_00001}">
                    <extension>
                        <m:HBox class="header-wrap" justifyContent="SpaceBetween">
                            <m:HBox class="header" alignItems="Center">
                                <m:Title level="H3" text="{i18n>LABEL_28033}" class="sapUiSmallMarginEnd" />
                                <m:Text text="total {/dialog/totalCount}" />
                            </m:HBox>
                            <m:HBox>
                                <m:Button icon="sap-icon://excel-attachment" text="Excel" press=".onPressDetailExcelDownload" />
                            </m:HBox>
                        </m:HBox>
                    </extension>
                    <columns>
                        <Column width="160px" filterProperty="OrgtxD" sortProperty="OrgtxD">
                            <m:Label text="{i18n>LABEL_28034}"/>
                            <template>
                                <m:Text text="{OrgtxD}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="160px" filterProperty="Orgtx" sortProperty="Orgtx">
                            <m:Label text="{i18n>LABEL_00224}"/>
                            <template>
                                <m:Text text="{Orgtx}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Ename" sortProperty="Ename">
                            <m:Label text="{i18n>LABEL_00210}"/>
                            <template>
                                <m:Link text="{Ename}" class="showLine" width="100%" textAlign="Center" press=".onPressEmployeeRow" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Zzjikgbtx" sortProperty="Zzjikgbtx">
                            <m:Label text="{i18n>LABEL_00215}"/>
                            <template>
                                <m:Text text="{Zzjikgbtx}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Zzjikchtx" sortProperty="Zzjikchtx">
                            <m:Label text="{i18n>LABEL_00217}"/>
                            <template>
                                <m:Text text="{Zzjikchtx}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="110px" filterProperty="Datum" sortProperty="Datum">
                            <m:Label text="{i18n>LABEL_00170}"/>
                            <template>
                                <m:Text text="{path: 'Datum', type: 'sap.ui.yesco.mvc.model.type.Date'}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="60px" filterProperty="Daytx" sortProperty="Daytx">
                            <m:Label text="{i18n>LABEL_06102}"/>
                            <template>
                                <m:Text text="{Daytx}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="60px" filterProperty="Offyn" sortProperty="Offyn">
                            <m:Label text="{i18n>LABEL_06103}"/>
                            <template>
                                <m:Text text="{Offyn}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Bashr" sortProperty="Bashr">
                            <m:Label text="{i18n>LABEL_28054}"/>
                            <template>
                                <m:Text text="{Bashr}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Addhr" sortProperty="Addhr">
                            <m:Label text="{i18n>LABEL_28055}"/>
                            <template>
                                <m:Text text="{Addhr}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Dedhr" sortProperty="Dedhr">
                            <m:Label text="{i18n>LABEL_01123}"/>
                            <template>
                                <m:Text text="{Dedhr}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Holhr" sortProperty="Holhr">
                            <m:Label text="{i18n>LABEL_13028}"/>
                            <template>
                                <m:Text text="{Holhr}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Nomtotc" sortProperty="Nomtotc">
                            <m:Label text="{i18n>LABEL_28056}"/>
                            <template>
                                <m:Text text="{Nomtotc}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                        <Column width="80px" filterProperty="Holtotc" sortProperty="Holtotc">
                            <m:Label text="{i18n>LABEL_28057}"/>
                            <template>
                                <m:Text text="{Holtotc}" width="100%" textAlign="Center" wrapping="false" class="tableRow_Text" />
                            </template>
                        </Column>
                    </columns>
                </Table>
            </m:VBox>
        </m:content>
        <m:buttons>
            <m:Button text="{i18n>LABEL_00115}" press=".onPressDetail1DialogClose"/>
        </m:buttons>
    </m:Dialog>
</core:FragmentDefinition>