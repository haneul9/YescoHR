<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <HBox>
        <IconTabBar id="profileTabBar" class="type-b" selectedKey="{/employee/tab/selectedKey}" expandable="false" items="{/employee/tab/list}">
            <layoutData>
                <FlexItemData growFactor="1"/>
            </layoutData>
            <items>
                <IconTabFilter text="{Menu1}" key="{Menuc1}">
                    <customData>
                        <core:CustomData key="mid" value="{Menuc1}" />
                    </customData>
                </IconTabFilter>
            </items>
        </IconTabBar>
    </HBox>
</core:FragmentDefinition>