<core:FragmentDefinition xmlns="sap.ui.table"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core">
    <m:Dialog title="{i18n>LABEL_40024}" contentWidth="1200px" contentHeight="570px" draggable="true">
        <m:content>
            <m:VBox class="dialog-content-table" busyIndicatorDelay="0" busy="{/busy/Dialog}">
                <Table width="40%" selectionMode="None" columnHeaderHeight="35" rowHeight="34" rows="{/dialog/work/list}" visibleRowCount="{/dialog/work/rowCount}" noData="{i18n>MSG_00001}">
                    <extension>
                        <m:HBox class="header-wrap" justifyContent="SpaceBetween">
                            <m:HBox class="header" alignItems="Center">
                                <m:Title level="H3" text="{i18n>LABEL_40001}" class="sapUiSmallMarginEnd" />
                            </m:HBox>
                        </m:HBox>
                    </extension>
                    <columns>
                        <Column width="50%">
                            <m:Label text="{i18n>LABEL_40004}"/>
                            <template>
                                <m:TimePicker value="{Beguz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                        <Column width="auto">
                            <m:Label text="{i18n>LABEL_40005}"/>
                            <template>
                                <m:TimePicker value="{Enduz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                    </columns>
                </Table>
                <Table width="100%" selectionMode="None" columnHeaderHeight="35" rowHeight="34" rows="{/dialog/legal/list}" visibleRowCount="{/dialog/legal/rowCount}" noData="{i18n>MSG_00001}">
                    <extension>
                        <m:HBox class="header-wrap" justifyContent="SpaceBetween">
                            <m:HBox class="header" alignItems="Center">
                                <m:Title level="H3" text="{i18n>LABEL_40002}" class="sapUiSmallMarginEnd" />
                            </m:HBox>
                        </m:HBox>
                    </extension>
                    <columns>
                        <Column width="20%">
                            <m:Label text="{i18n>LABEL_40004}"/>
                            <template>
                                <m:TimePicker value="{Beguz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                        <Column width="20%">
                            <m:Label text="{i18n>LABEL_40005}"/>
                            <template>
                                <m:TimePicker value="{Enduz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                        <Column width="15%">
                            <m:Label text="{i18n>LABEL_00330}"/>
                            <template>
                                <m:Text text="{Todo1}" width="100%" textAlign="Center" />
                            </template>
                        </Column>
                        <Column width="auto">
                            <m:Label text="{i18n>LABEL_40006}"/>
                            <template>
                                <m:Text text="{Todo2}" width="100%" textAlign="Center" />
                            </template>
                        </Column>
                    </columns>
                </Table>
                <Table width="100%" selectionMode="None" fixedBottomRowCount="1" columnHeaderHeight="35" rowHeight="34" rows="{/dialog/extra/list}" visibleRowCount="{/dialog/extra/rowCount}" noData="{i18n>MSG_00001}">
                    <extension>
                        <m:HBox class="header-wrap" justifyContent="SpaceBetween">
                            <m:HBox class="header" alignItems="Center">
                                <m:Title level="H3" text="{i18n>LABEL_40003}" class="sapUiSmallMarginEnd" />
                            </m:HBox>
                        </m:HBox>
                    </extension>
                    <columns>
                        <Column width="20%">
                            <m:Label text="{i18n>LABEL_40004}"/>
                            <template>
                                <m:TimePicker value="{Beguz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                        <Column width="20%">
                            <m:Label text="{i18n>LABEL_40005}"/>
                            <template>
                                <m:TimePicker value="{Enduz}" placeholder="HH:mm" valueFormat="HHmm" displayFormat="HH:mm" busyIndicatorDelay="0" busy="{/busy/Input}" class="sapIcon_Time" />
                            </template>
                        </Column>
                        <Column width="15%">
                            <m:Label text="{i18n>LABEL_00330}"/>
                            <template>
                                <m:Text text="{Todo1}" width="100%" textAlign="Center" />
                            </template>
                        </Column>
                        <Column width="auto">
                            <m:Label text="{i18n>LABEL_40006}"/>
                            <template>
                                <m:Input value="{Todo2}" busyIndicatorDelay="0" busy="{/busy/Input}" class="small-table-input" />
                            </template>
                        </Column>
                    </columns>
                </Table>
            </m:VBox>
        </m:content>
        <m:buttons>
            <m:Button text="{i18n>LABEL_00349}" type="Emphasized" busyIndicatorDelay="0" busy="{/busy/Button}" press=".onPressDialogConfirm" />
            <m:Button text="{i18n>LABEL_00115}" press=".onDialogClose" busyIndicatorDelay="0" busy="{/busy/Button}" class="cancel" />
        </m:buttons>
    </m:Dialog>
</core:FragmentDefinition>