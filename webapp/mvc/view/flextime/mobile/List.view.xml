  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:mobile="sap.ui.yesco.control.mobile">
  <mobile:Page navButtonPress=".onNavBack">
    <VBox class="contents">
      <mobile:ScrollContainer>
        <core:Fragment fragmentName="sap.ui.yesco.mvc.view.flextime.mobile.fragment.EmpInfo" type="XML" />
        <core:Fragment fragmentName="sap.ui.yesco.mvc.view.flextime.mobile.fragment.YearPlan" type="XML" />
        <core:Fragment fragmentName="sap.ui.yesco.mvc.view.flextime.mobile.fragment.Summary" type="XML" />
      </mobile:ScrollContainer>
    </VBox>
  </mobile:Page>
</mvc:View>