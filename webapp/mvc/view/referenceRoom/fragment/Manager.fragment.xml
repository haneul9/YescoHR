<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:table="sap.ui.table">
  <Dialog id= "managerSettingsDialog" title="{i18n>LABEL_29008}" contentWidth="500px" contentHeight="auto">
    <content>
      <table:Table id="managerTable" width="100%" rows="{/ManagerList}" visibleRowCount="3" selectionMode="None" rowHeight="44" columnHeaderHeight="45" cellClick=".onManagerClick">
        <table:columns>
          <table:Column width="60%">
            <Label text="{i18n>LABEL_00226}"/>
            <table:template>
              <Text text="{Atext}" wrapping="false" />
            </table:template>
          </table:Column>
          <table:Column>
            <Label text="{i18n>LABEL_29008}"/>
            <table:template>
              <Text text="{Zname}" wrapping="false" />
            </table:template>
          </table:Column>
        </table:columns>
      </table:Table>
    </content>
    <endButton>
      <Button text="{i18n>LABEL_00115}" press=".onClick" class="cancel" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>