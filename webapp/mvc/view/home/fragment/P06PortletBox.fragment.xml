            <customData>
              <core:CustomData key="popover" value="{= ${counts/Cnt08} === '0' ? 'N' : 'Y' }" writeToDom="true" />
              <core:CustomData key="table-key" value="k{counts/Cod08}" writeToDom="true" />
            </customData>
          </ObjectStatus>
        </VBox>
        <HBox justifyContent="End" class="mr-18-px">
          <layoutData>
            <grid:GridItemLayoutData gridColumn="span 4" />
          </layoutData>
          <Text text="{i18n>MSG_28003}" class="font-9-px ls-small" />
        </HBox>
      </grid:CSSGrid>
    </HBox>
  </VBox>
</core:FragmentDefinition>