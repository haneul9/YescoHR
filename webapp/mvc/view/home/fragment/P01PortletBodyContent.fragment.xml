<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <!-- 개인정보 -->
  <HBox width="100%">
    <VBox width="45%" class="my-info">
      <Image src="{sessionModel>/Photo}" mode="Background" backgroundPosition="center top" class="member-photo" width="120px" height="120px" />
      <Text text="{Ename}" class="name"/>
      <Text text="{Zzjiktlt}" class="position"/>
      <Text text="{Orgtx}" class="team" />
      <Text text="{Stltx}" class="org" />
    </VBox>
    <VBox width="55%" class="my-info-summary">
      <Label text="{i18n>LABEL_01101}" />
      <Text text="{Zyymm1}" />
      <Text text="{Period1}" class="date"/>
      <Label text="{i18n>LABEL_01102}" />
      <Text text="{Zyymm2}" />
      <Text text="{Period2}" class="date"/>
      <Label text="{i18n>LABEL_01103}" />
      <Text text="{Zyymm3}" />
      <Text text="{Period3}" class="date"/>
    </VBox>
  </HBox>
</core:FragmentDefinition>