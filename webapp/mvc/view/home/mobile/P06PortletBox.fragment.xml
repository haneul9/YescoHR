      <custom:Title text="{title}" visible="{= !${hideTitle} }">
        <custom:layoutData>
          <FlexItemData styleClass="portlet-header-title" />
        </custom:layoutData>
      </custom:Title>
      <custom:DatePicker dateValue="{ path: 'selectedDate', type: 'sap.ui.yesco.mvc.model.type.DateWeekday' }" change=".onChangeSelectedDate" pickOnly="true" class="icon-dropdown" />
