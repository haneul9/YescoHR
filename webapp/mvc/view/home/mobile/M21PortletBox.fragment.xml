<core:FragmentDefinition xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:custom="sap.ui.yesco.control">
  <!-- 임원용 : 인원현황 -->
  <VBox tooltip="{tooltip}" busy="{busy}" busyIndicatorDelay="0">
    <HBox class="portlet-header" visible="{= !${hideTitle} || ${switchable} }">
      <custom:Title text="{title}" visible="{= !${hideTitle} }">
        <custom:layoutData>
          <FlexItemData styleClass="portlet-header-title" />
        </custom:layoutData>
      </custom:Title>
      <Text text="{sessionModel>/Orgtx}, {i18n>LABEL_01181}" class="desc-txt">
        <layoutData>
          <FlexItemData styleClass="portlet-header-description" />
        </layoutData>
      </Text>
      <Text text="">
        <layoutData>
          <FlexItemData growFactor="1" />
        </layoutData>
      </Text>
      <Button visible="{hasLink}" icon="sap-icon://add" type="Transparent" class="icon-button portlet-link-icon" press=".onPressLink" />
    </HBox>
    <HBox class="portlet-body">
      <VBox class="statistic-sum" binding="{/contents/0}">
        <custom:StatisticNumber path="Cnt01" large="true" press=".onPressCount">
          <custom:layoutData>
            <FlexItemData styleClass="statistic-sum-total" />
          </custom:layoutData>
          <custom:customData>
            <core:CustomData key="OData" value="H" />
            <core:CustomData key="Headty" value="A" />
            <core:CustomData key="Discod" value="{Cod01}" />
          </custom:customData>
        </custom:StatisticNumber>
        <custom:StatisticNumber prefix="{i18n>LABEL_01213} : " path="Cnt02" press=".onPressCount">
          <custom:layoutData>
            <FlexItemData styleClass="statistic-sum-employee" />
          </custom:layoutData>
          <custom:customData>
            <core:CustomData key="OData" value="H" />
            <core:CustomData key="Headty" value="A" />
            <core:CustomData key="Discod" value="{Cod02}" />
          </custom:customData>
        </custom:StatisticNumber>
        <custom:StatisticNumber prefix="{i18n>LABEL_01212} : " path="Cnt03" press=".onPressCount">
          <custom:layoutData>
            <FlexItemData styleClass="statistic-sum-executive" />
          </custom:layoutData>
          <custom:customData>
            <core:CustomData key="OData" value="H" />
            <core:CustomData key="Headty" value="A" />
            <core:CustomData key="Discod" value="{Cod03}" />
          </custom:customData>
        </custom:StatisticNumber>
      </VBox>
      <VBox class="statistic-box">
        <HBox class="statistic-box-header">
          <Label text=" ">
            <layoutData>
              <FlexItemData styleClass="statistic-box-header-title" />
            </layoutData>
          </Label>
          <Label text="{i18n>LABEL_01211}">
            <layoutData>
              <FlexItemData styleClass="statistic-box-header-title" />
            </layoutData>
          </Label>
          <Label text="{i18n>LABEL_01213}">
            <layoutData>
              <FlexItemData styleClass="statistic-box-header-title" />
            </layoutData>
          </Label>
          <Label text="{i18n>LABEL_01212}">
            <layoutData>
              <FlexItemData styleClass="statistic-box-header-title" />
            </layoutData>
          </Label>
        </HBox>
        <HBox class="statistic-box-row row-1" binding="{/contents/1}">
          <Label text="{i18n>LABEL_01227}">
            <layoutData>
              <FlexItemData styleClass="statistic-box-row-title" />
            </layoutData>
          </Label>
          <!-- 전년말 : 전체 -->
          <custom:StatisticNumber path="Cnt01" press=".onPressCount">
            <custom:layoutData>
              <FlexItemData styleClass="statistic-box-row-data" />
            </custom:layoutData>
            <custom:customData>
              <core:CustomData key="OData" value="H" />
              <core:CustomData key="Headty" value="A" />
              <core:CustomData key="Discod" value="{Cod01}" />
            </custom:customData>
          </custom:StatisticNumber>
          <!-- 전년말 : 직원 -->
          <custom:StatisticNumber path="Cnt02" press=".onPressCount">
            <custom:layoutData>
              <FlexItemData styleClass="statistic-box-row-data" />
            </custom:layoutData>
            <custom:customData>
              <core:CustomData key="OData" value="H" />
              <core:CustomData key="Headty" value="A" />
              <core:CustomData key="Discod" value="{Cod02}" />
            </custom:customData>
          </custom:StatisticNumber>
          <!-- 전년말 : 임원 -->
          <custom:StatisticNumber path="Cnt03" press=".onPressCount">
            <custom:layoutData>
              <FlexItemData styleClass="statistic-box-row-data" />
            </custom:layoutData>
            <custom:customData>
              <core:CustomData key="OData" value="H" />
              <core:CustomData key="Headty" value="A" />
              <core:CustomData key="Discod" value="{Cod03}" />
            </custom:customData>
          </custom:StatisticNumber>
        </HBox>
        <HBox class="statistic-box-row row-2" binding="{/contents/2}">
          <Label text="{i18n>LABEL_01228}">
            <layoutData>
              <FlexItemData styleClass="statistic-box-row-title" />
            </layoutData>
          </Label>
          <!-- 입사자 : 전체 -->
          <custom:StatisticNumber path="Cnt01" press=".onPressCount">
