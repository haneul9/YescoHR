<mvc:View controllerName="sap.ui.yesco.mvc.controller.accountChange.mobile.AccountChange"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:mobile="sap.ui.yesco.control.mobile">
  <mobile:Page navButtonPress=".onNavBack">
    <VBox class="contents">
      <core:Fragment fragmentName="sap.ui.yesco.fragment.mobile.OverviewListHeader" type="XML" />
      <mobile:ScrollContainer>
        <mobile:List items="{path: '/Data'}">
          <mobile:CustomListItem press=".onSelectRow">
            <mobile:ApprovalRequestListItemBox>
              <mobile:ListCSSGrid gridTemplateColumns="2fr 2fr">
                <Text text="{Accttx}" class="emphasized" />
                <Text text="{Banka}"/>
                <Text text="{ path: 'Begym', formatter: '.formatYear' }"/>
                <Text text="{Bankn}"/>
              </mobile:ListCSSGrid>
            </mobile:ApprovalRequestListItemBox>
          </mobile:CustomListItem>
        </mobile:List>
      </mobile:ScrollContainer>
    </VBox>
    <mobile:footer>
      <mobile:ApprovalRequestListFooterToolbar pressRequestNew=".onClick" />
    </mobile:footer>
  </mobile:Page>
</mvc:View>