<core:FragmentDefinition xmlns="sap.m"
    xmlns:grid="sap.ui.layout.cssgrid"
    xmlns:core="sap.ui.core"
    xmlns:custom="sap.ui.yesco.control">
    <VBox class="vCardBox" busyIndicatorDelay="0" busy="{/busy}">
        <HBox alignItems="Center" class="table-toolbar">
            <Title level="H2" text="{i18n>LABEL_00129}" />
        </HBox>
        <grid:CSSGrid gridTemplateColumns="1fr 3fr 1fr 3fr" gridGap="1px 0" class="form-grid">
            <Label text="{i18n>LABEL_25008}" />
            <VBox>
                <ComboBox items="{/MaintainType}" selectedKey="{/FormData/Appty}" change="onMaintainType" editable="{/FormData/Fixed}" class="required">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </VBox>
            <Label text="{i18n>LABEL_25009}" />
            <VBox>
                <ComboBox items="{/AppDept}" selectedKey="{/FormData/Payorg}" editable="{/FormData/Fixed}" class="required">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </VBox>
            <Label text="{i18n>LABEL_25010}"/>
            <HBox>
                <custom:DatePicker dateValue="{ path: '/FormData/Expdt', type: 'sap.ui.yesco.mvc.model.type.Date' }" width="250px" editable="{= !${/FormData/Fixed} }" class="sapUiTinyMarginEnd required"/>
                <Text text="{i18n>MSG_25002}"/>
                <layoutData>
                    <grid:GridItemLayoutData gridColumn="2 / 5"/>
                </layoutData>
            </HBox>
            <Label text="{i18n>LABEL_25005}"/>
            <VBox>
                <Input value="{/FormData/Carno}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25011}"/>
            <VBox>
                <Input value="{/FormData/Carty}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25012}"/>
            <VBox>
                <Input value="{/FormData/Cc}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25013}"/>
            <VBox>
                <Input value="{/FormData/Caryr}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25014}"/>
            <VBox>
                <custom:DatePicker dateValue="{ path: '/FormData/Cardt', type: 'sap.ui.yesco.mvc.model.type.Date' }" width="250px" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25015}"/>
            <VBox>
                <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Insu} }" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <VBox>
                <layoutData>
                    <grid:GridItemLayoutData gridColumn="1 / 5"/>
                </layoutData>
            </VBox>
            <Label text="{i18n>LABEL_25016}"/>
            <VBox>
                <Input value="{/FormData/Lnmhg}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_25017}"/>
            <VBox>
                <ComboBox items="{/LicenseType}" selectedKey="{/FormData/Idtype}" change=".onShcoolList" editable="{/FormData/Fixed}" class="required">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </VBox>
            <VBox>
                <layoutData>
                    <grid:GridItemLayoutData gridColumn="1 / 5"/>
                </layoutData>
            </VBox>
            <Label text="{i18n>LABEL_25007}" />
            <VBox >
                <ComboBox items="{/PayType}" selectedKey="{/FormData/Payty}" change=".onShcoolList" editable="{/FormData/Fixed}" class="required">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </VBox>
            <Label text="{i18n>LABEL_25018}" />
            <HBox>
                <Text text="{/FormData/Ename}" class="sapUiTinyMarginEnd"/>
                <Text text="{i18n>MSG_25003}"/>
            </HBox>
            <Label text="{i18n>LABEL_25019}" />
            <VBox >
                <ComboBox items="{/BankList}" selectedKey="{/FormData/Bankl}" editable="{/FormData/Fixed}" class="required">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </VBox>
            <Label text="{i18n>LABEL_25020}" />
            <VBox >
                <Input value="{/FormData/Bankn}" editable="{/FormData/Fixed}" class="required"/>
            </VBox>
            <Label text="{i18n>LABEL_00163}" />
            <VBox >
                <Input value="{/FormData/Zbigo}" editable="{/FormData/Fixed}" />
                <layoutData>
                    <grid:GridItemLayoutData gridColumn="2 / 5"/>
                </layoutData>
            </VBox>
        </grid:CSSGrid>
    </VBox>
</core:FragmentDefinition>