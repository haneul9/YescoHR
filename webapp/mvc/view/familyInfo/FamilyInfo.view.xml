<mvc:View controllerName="sap.ui.yesco.mvc.controller.familyInfo.FamilyInfo"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core">
	<Page showHeader="false">
		<content>
			<VBox>
				<HBox justifyContent="SpaceBetween" class="breadcrumbs">
					<core:Fragment fragmentName="sap.ui.yesco.fragment.app.Breadcrumbs" type="XML" />
					<HBox>
						<Button text="{i18n>LABEL_00121}" type="Emphasized" press=".onClick" class="sapUiTinyMarginEnd" />
						<Button icon="sap-icon://print" press=".FragmentEvent.onPagePrint" text="{i18n>LABEL_00131}" class="sapUiTinyMarginEnd" />
						<Button icon="sap-icon://sys-help" text="{i18n>LABEL_00132}" />
					</HBox>
				</HBox>
				<HBox justifyContent="SpaceAround" class="sapUiMediumMarginBottom">
					<core:Fragment fragmentName="sap.ui.yesco.fragment.TargetInfo" type="XML" />
					<core:Fragment fragmentName="sap.ui.yesco.mvc.view.familyInfo.fragment.TotalFamily" type="XML" />
					<core:Fragment fragmentName="sap.ui.yesco.mvc.view.familyInfo.fragment.SearchCondition" type="XML" />
				</HBox>
				<VBox class="vCardBox">
					<core:Fragment fragmentName="sap.ui.yesco.fragment.OverviewTableHeader" type="XML" />
					<core:Fragment fragmentName="sap.ui.yesco.mvc.view.familyInfo.fragment.ApplyListTable" type="XML" />
				</VBox>
			</VBox>
		</content>
	</Page>
</mvc:View>