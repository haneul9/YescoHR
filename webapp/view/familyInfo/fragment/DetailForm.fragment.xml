<core:FragmentDefinition xmlns="sap.m"
    xmlns:grid="sap.ui.layout.cssgrid"
    xmlns:core="sap.ui.core">
    <VBox class="vCardBox" busyIndicatorDelay="0" busy="{/busy}">
        <HBox>
            <Title level="H2" text="{i18n>LABEL_00129}" />
        </HBox>
        <grid:CSSGrid gridTemplateColumns="1fr 3fr 1fr 3fr" gridGap="8px">
            <Label text="{i18n>LABEL_00210}" />
            <HBox class="mb-0 ">
                <Text text="{i18n>LABEL_05004}"/>
                <Input value="{/FormData/Lnmhg}" valueState="Warning" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
                <Text text="{i18n>LABEL_05005}"/>
                <Input value="{/FormData/Fnmhg}" valueState="Warning" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            </HBox>
            <Label text="{i18n>LABEL_05006}" />
                <Input value="{/FormData/Regno}" valueState="Warning" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05007}"/>
            <ComboBox valueState="Warning" items="{/Relations}" selectedKey="{/FormData/Kdsvh}" change=".onShcoolList" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }">
                <core:Item text="{Atext}" key="{Auspr}" />
            </ComboBox>
            <Label text="{i18n>LABEL_05008}"/>
            <ComboBox valueState="Warning" change=".onGrade" items="{/Gender}" selectedKey="{/FormData/Fasex}" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }">
                <core:Item text="{Ztext}" key="{Zcode}" />
            </ComboBox>
            <Label text="{i18n>LABEL_05009}"/>
            <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Dptid} ? true : false }" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05010}"/>
            <HBox >
                <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Hndid} ? true : false }" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
                <ComboBox change=".onYearsSelect" valueState="{/DisabCheck}" items="{/Disability}" selectedKey="{/FormData/Hndcd}" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }">
                    <core:Item text="{Ztext}" key="{Zcode}" />
                </ComboBox>
            </HBox>
            <Label text="{i18n>LABEL_05011}"/>
            <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Livid} ? true : false }" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05012}"/>
            <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Helid} ? true : false }" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05013}"/>
            <ComboBox change=".onYearsSelect" valueState="{/SupCheck}" items="{/Support}" selectedKey="{/FormData/Dptyp}" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }">
                <core:Item text="{Dptyx}" key="{Dptyp}" />
            </ComboBox>
            <Label text="{i18n>LABEL_05014}"/>
            <CheckBox select=".onCheckBox" selected="{= !!${/FormData/Famid} ? true : false }" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05015}"/>
            <DatePicker class="sapIcon_Date" valueState="Warning" dateValue="{/FormData/Begda}" width="250px" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
            <Label text="{i18n>LABEL_05016}"/>
            <DatePicker class="sapIcon_Date" valueState="Warning" dateValue="{/FormData/Endda}" width="250px" editable="{= ${/FormData/ZappStatAl} === '10' || !${/FormData/ZappStatAl} }" />
        </grid:CSSGrid>
    </VBox>
</core:FragmentDefinition>